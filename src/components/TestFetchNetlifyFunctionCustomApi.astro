import {parseHTML} from 'linkedom';

const titreUne =  await fetch("https://my-butler.netlify.app/.netlify/functions/fetch-title-api")
  .then((response) => {
    return response.json();
  })
  .then((htmlText) => {
    const {document: documentParsed} = parseHTML(htmlText);
    // You can now even select part of that html as you would in the regular DOM
    const titreUne = documentParsed.querySelector("h2")?.textContent;
    console.log("üê†üê†", titreUne);
  })
  .catch((error) => console.error(error));

